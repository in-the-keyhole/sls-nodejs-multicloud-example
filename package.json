  },
  "devDependencies": {
    "@types/node": "^17.0.34",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "serverless-azure-functions": "^2.1.1",
    "serverless-offline": "^8.3.1",
    "serverless-webpack": "^5.7.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-node-externals": "^3.0.0"
  }
}
